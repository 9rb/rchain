new stdout(`rho:io:stdout`) in {

  // prints "b" because lists are 0-indexed
  stdout!(["a", "b", "c", "d"].nth(1)) |


  // prints ["b", "c"]
  // lower bound is inclusive, upper is exclusive
  stdout!(["a", "b", "c", "d"].slice(1, 3)) |

  // Test length
  stdout!(["a", "b", "c", "d"].length())
}
